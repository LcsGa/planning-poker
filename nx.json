  },
  "implicitDependencies": {
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    ".eslintrc.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/nx-cloud",
      "options": {
        "cacheableOperations": ["build", "lint"],
        "accessToken": "NWNmZDM2N2ItNDZhNi00N2M1LTgwNTMtODMyM2QyZWNiNTAxfHJlYWQtd3JpdGU="
      }
    }
  },
  "generators": {
